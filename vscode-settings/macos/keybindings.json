[
  // redo
  {
    "key": "alt+/",
    "command": "redo"
  },
  {
    "key": "ctrl+shift+/",
    "command": "redo"
  },
  // ctrl + u : カーソルから先頭まで削除
  {
    "key": "ctrl+u",
    "command": "deleteAllLeft"
  },

  // タブ切り替えを変更
  // ctrl + tab : 次のタブ
  // ctrl + shift + tab : 前のタブ
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "ctrl+pagedown",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "ctrl+pageup",
    "command": "-workbench.action.previousEditor"
  },

  // shift + space : 補完表示トリガーを変更
  {
    "key": "shift+space",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
  },

  // ctrl + k : ターミナルでも有効化
  {
    "key": "ctrl+k",
    "command": "emacs-mcx.killLine",
    "when": "terminalFocus"
  },

  // alt + k : paredit-kill
  {
    "key": "alt+k",
    "command": "emacs-mcx.paredit.killSexp",
    "when": "editorTextFocus && !config.emacs-mcx.useMetaPrefixMacCmd"
  },

  // alt + h : kill-word
  {
    "key": "alt+h",
    "command": "emacs-mcx.backwardKillWord",
    "when": "editorTextFocus && !config.emacs-mcx.useMetaPrefixMacCmd && !editorReadonly"
  },
  {
    "key": "alt+h",
    "command": "emacs-mcx.backwardKillWord",
    "when": "config.emacs-mcx.useMetaPrefixMacCmd && editorTextFocus && !editorReadonly"
  },

  // エディタに何も表示していないときでもターミナルを表示(ターミナルフォーカス移動より前に定義)
  // ctrl + [ : ターミナル表示
  {
    "key": "ctrl+[",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+[BracketLeft]",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  // ターミナル切替からフォーカス移動に変更
  // ctrl + [ : ターミナルにフォーカスを移動
  {
    "key": "ctrl+[",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+[",
    "command": "workbench.action.terminal.focus",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+[BracketLeft]",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+[BracketLeft]",
    "command": "workbench.action.terminal.focus",
    "when": "editorFocus"
  },
  // ctrl + shift + [ : 新しいターミナルを開く
  {
    "key": "ctrl+shift+[",
    "command": "workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+shift+[BracketLeft]",
    "command": "workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },

  // シンボルの移動（前方）を変更
  // before : f7
  // after  : f3
  {
    "key": "f3",
    "command": "editor.action.wordHighlight.next",
    "when": "editorTextFocus && hasWordHighlights"
  },
  {
    "key": "f7",
    "command": "-editor.action.wordHighlight.next",
    "when": "editorTextFocus && hasWordHighlights"
  },
  {
    "key": "f3",
    "command": "editor.action.diffReview.next",
    "when": "isInDiffEditor"
  },
  {
    "key": "f7",
    "command": "-editor.action.diffReview.next",
    "when": "isInDiffEditor"
  },

  // シンボルの移動（後方）を変更
  // before : shift + f7
  // after  : shift + f3
  {
    "key": "shift+f3",
    "command": "editor.action.wordHighlight.prev",
    "when": "editorTextFocus && hasWordHighlights"
  },
  {
    "key": "shift+f7",
    "command": "-editor.action.wordHighlight.prev",
    "when": "editorTextFocus && hasWordHighlights"
  },
  {
    "key": "shift+f3",
    "command": "editor.action.diffReview.prev",
    "when": "isInDiffEditor"
  },
  {
    "key": "shift+f7",
    "command": "-editor.action.diffReview.prev",
    "when": "isInDiffEditor"
  },

  // フォーカスワードの検索（ターミナル・エディタ）の削除
  // remove : f3
  // remove : shift + f3
  {
    "key": "f3",
    "command": "-workbench.action.terminal.findNext",
    "when": "terminalFindFocused && terminalProcessSupported || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+f3",
    "command": "-workbench.action.terminal.findPrevious",
    "when": "terminalFindFocused && terminalProcessSupported || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "f3",
    "command": "-editor.action.nextMatchFindAction",
    "when": "editorFocus"
  },
  {
    "key": "shift+f3",
    "command": "-editor.action.previousMatchFindAction",
    "when": "editorFocus"
  },

  // ビルドタスクの実行の変更
  // default (not removed) : cmd + shift + b
  // after                 : f7
  {
    "key": "f7",
    "command": "workbench.action.tasks.build"
  },

  // サイドバーの表示の切り替えの変更
  // default (not removed) : cmd + b
  // after                 : ctrl + shift + b
  {
    "key": "ctrl+shift+b",
    "command": "workbench.action.toggleSidebarVisibility"
  },

  // plantUMLのプレビュー表示を変更
  // before : alt + d
  // after  : cmd + shift + v
  {
    "key": "cmd+shift+v",
    "command": "plantuml.preview",
    "when": "!notebookEditorFocused && editorLangId == 'plantuml'"
  },
  {
    "key": "alt+d",
    "command": "-plantuml.preview"
  },

  // Markdown Preview Enhanced プレビューを横に表示の変更
  // default (not removed) : cmd + k v
  // after                 : cmd + shift + v
  // {
  //     "key": "shift+cmd+v",
  //     "command": "markdown-preview-enhanced.openPreviewToTheSide",
  //     "when": "editorLangId == 'markdown'"
  // },
  // Markdownプレビューの表示
  // shift + cmd + v：削除
  // {
  //     "key": "shift+cmd+v",
  //     "command": "-markdown.showPreview",
  //     "when": "!notebookEditorFocused && editorLangId == 'markdown'"
  // },

  // shift + cmd + v : Markdownプレビューを横に表示
  {
    "key": "shift+cmd+v",
    "command": "markdown.showPreviewToSide",
    "when": "!notebookEditorFocused && editorLangId == 'markdown'"
  },

  // 選択範囲のフォーマット
  // cmd + k cmd + v：デフォルト（未削除）
  // shift + alt + k：変更後
  // Bookmarksの選択の縮小を削除
  {
    "key": "shift+alt+k",
    "command": "editor.action.formatSelection",
    "when": "editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+k",
    "command": "-bookmarks.shrinkSelection",
    "when": "editorTextFocus"
  },

  // Markdown Table とかぶっているので削除(Markdown All in One)
  // {
  //   "key": "tab",
  //   "command": "-markdown.extension.onTabKey",
  //   "when": "editorTextFocus && !editorHasMultipleSelections && !editorReadonly && !editorTabMovesFocus && !hasOtherSuggestions && !hasSnippetCompletions && !inSnippetMode && !inlineSuggestionVisible && !suggestWidgetVisible && editorLangId == 'markdown'"
  // },

  // git 選択範囲のステージング
  {
    "key": "ctrl+x ctrl+q",
    "command": "git.stageSelectedRanges",
    "when": "isInDiffEditor && !operationInProgress"
  },
  {
    "key": "ctrl+x ctrl+z",
    "command": "git.unstageSelectedRanges",
    "when": "isInDiffEditor && !operationInProgress"
  },
  {
    "key": "ctrl+x ctrl+r",
    "command": "git.revertSelectedRanges",
    "when": "isInDiffEditor && !operationInProgress"
  },

  // Quick Open Rails
  {
    "key": "alt+t",
    "command": "quickOpenRails.open",
    "when": "editorFocus"
  },
  // Show Hover
  // add: ctrl + q
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenNavigateNextInViewPicker",
    "when": "inQuickOpen && inViewsPicker"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenView"
  },
  {
    "key": "ctrl+q",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+q",
    "command": "workbench.action.terminal.focusHover",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalIsOpen || terminalFocus && terminalProcessSupported || terminalHasBeenCreated && terminalTabsFocus || terminalIsOpen && terminalTabsFocus || terminalProcessSupported && terminalTabsFocus"
  },
  {
    "key": "ctrl+q",
    "command": "editor.debug.action.showDebugHover",
    "when": "editorTextFocus && inDebugMode"
  },
  {
    "key": "ctrl+q",
    "command": "list.showHover",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused"
  }
  // EOF
]
