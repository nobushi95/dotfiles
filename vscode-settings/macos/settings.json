{
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "security.workspace.trust.untrustedFiles": "open",

  // git
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",

  // editor font
  "editor.fontFamily": "'UDEV Gothic', Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "HackGen, Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "'Ricty Diminished', Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "SF Mono Square, MyricaM M, Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "SF Mono Square, MyricaM M, Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "MyricaM M, Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "Menlo, Osaka-Mono, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "Osaka-Mono, Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "Source Han mono, Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "'Source Han Code JP', Menlo, Monaco, 'Courier New', monospace",
  // "editor.fontFamily": "'源ノ角ゴシック Code JP', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "editor.lineHeight": 16, // フォントサイズ+1が目安

  // terminal font
  // "terminal.integrated.fontFamily": "MesloLGS NF", // for powerlelvel10k theme
  // "terminal.integrated.fontFamily": "HackGen35 Console NFJ",
  "terminal.integrated.fontFamily": "'UDEV Gothic 35NF'",
  "terminal.integrated.fontSize": 13,

  // debug console font
  "debug.console.fontSize": 13,

  // editor
  "editor.suggestSelection": "first",
  "editor.renderWhitespace": "boundary",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.minimap.maxColumn": 60,
  "editor.minimap.showSlider": "always",
  "editor.stickyScroll.enabled": true,
  "editor.guides.bracketPairs": true,

  // Formatting Toggleが自動生成
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,

  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.fixAll.eslint": "explicit"
  },

  // terminal
  "terminal.integrated.enableMultiLinePasteWarning": false,

  // window
  "window.commandCenter": true,

  // explorer
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // xaml ファイルの折りたたみ
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.xaml": "$(capture).xaml.cs"
  },

  // files
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },

  // workbench
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "material-icon-theme",

  // ワークベンチカラー設定
  "workbench.colorCustomizations": {
    // MinimapSlider
    "minimapSlider.background": "#ffffff17",
    "minimapSlider.hoverBackground": "#ffffff1b",
    "minimapSlider.activeBackground": "#ffffff20",
    // for bracket colorization
    "editorBracketHighlight.foreground1": "#FF7FFF", // 紫
    "editorBracketHighlight.foreground2": "#3F7FFF", // 青
    "editorBracketHighlight.foreground3": "#3FFF3F", // 緑
    "editorBracketHighlight.foreground4": "#FFFF00", // 黄
    "editorBracketHighlight.foreground5": "#FFBF3F", // 橙
    "editorBracketHighlight.foreground6": "#FF0000" // 赤
  },

  // インデントカラー
  "indentRainbow.colors": [
    "rgba(128,  64, 128, 0.5)", // 紫
    "rgba( 32,  64, 128, 0.5)", // 青
    "rgba( 32, 128,  32, 0.5)", // 緑
    "rgba(128, 128,  32, 0.5)", // 黄
    "rgba(128,  96,  32, 0.5)", // 橙
    "rgba(128,  64,  64, 0.5)" // 赤
  ],

  "multiCommand.commands": [
    {
      "command": "gotoNextCellAndFormat",
      "sequence": ["text-tables.gotoNextCell", "extension.table.formatCurrent"]
    },
    {
      "command": "gotoPreviousCellAndFormat",
      "sequence": [
        "text-tables.gotoPreviousCell",
        "extension.table.formatCurrent"
      ]
    }
  ],

  // モードごとの設定
  "[csv]": {
    // "editor.fontFamily": "Osaka-Mono",
    // "editor.fontFamily": "Ricty Diminished",
    // "editor.fontSize": 14
  },
  "[markdown]": {
    // デフォルト設定？
    // "editor.unicodeHighlight.ambiguousCharacters": false,
    // "editor.wordWrap": "on",
    // "editor.quickSuggestions": false,
    // フォント設定
    // "editor.fontFamily": "Ricty Diminished",
    // "editor.fontFamily": "Osaka-Mono",
    // "editor.fontFamily": "SF Mono Square",
    // "editor.fontFamily": "MyricaM M",
    // "editor.fontSize": 14
  },
  "[json]": {
    // "editor.defaultFormatter": "vscode.json-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    // "editor.defaultFormatter": "vscode.json-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascript]": {
    // "editor.maxTokenizationLineLength": 2500
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescript]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.rulers": [120],
    "editor.semanticHighlighting.enabled": true
  },

  // go
  "go.toolsManagement.autoUpdate": true,

  // typescript
  "typescript.updateImportsOnFileMove.enabled": "always",

  // past image
  "pasteImage.path": "${currentFileDir}/images",

  // git-graph
  "git-graph.repository.fetchAndPrune": true,
  "git-graph.repository.fetchAndPruneTags": true,

  // Live Server
  "liveServer.settings.donotShowInfoMsg": true,

  // draw-io
  "hediet.vscode-drawio.theme": "atlas",

  // one dark pro
  // "oneDarkPro.vivid": true,
  // "oneDarkPro.italic": false,
  // "oneDarkPro.bold": true,

  // For Path Intellisense
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,

  // Code Runner
  "code-runner.executorMap": {
    "typescript": "npx ts-node"
  },

  // prettier
  "prettier.singleQuote": true,
  // "prettier.jsxSingleQuote": true,

  // tabnine
  "tabnine.experimentalAutoImports": true
}
